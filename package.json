{
  "name": "react-quick-start",
  "version": "1.0.0",
  "description": "React quick Start UI",
  "author": "Platforms Development",
  "private": true,
  "main": "index.js",
  "scripts": {
    "install:io": "yarn install --ignore-optional --network-timeout 60000",
    "clean": "rimraf dist/*",
    "webp:max-heap-size": "webpack-dev-server --mode development --max_old_space_size=4096",
    "start": "yarn run webp:max-heap-size",
    "start:https": "set HTTPS=true&&yarn run start",
    "build:wp": "webpack --max_old_space_size=4096 --progress=false --profile --bail",
    "build": "yarn run clean && yarn run build:wp",
    "build:ci": "yarn run build",
    "test:without-coverage": "yarn run jest --logHeapUsage --maxWorkers=2 --config jest.config.js",
    "test:with-coverage": "yarn run test:without-coverage --coverage",
    "test:once": "jest",
    "test": "yarn run test:with-coverage",
    "test:watch": "yarn run test:without-coverage --watch"
  },
  "dependencies": {
    "@date-io/date-fns": "1.0.2",
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "d3": "^5.7.0",
    "date-fns": "^2.0.0-alpha.21",
    "jss": "^9.8.7",
    "material-ui-pickers": "^2.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dom-factories": "^1.0.2",
    "react-hot-loader": "^4.3.12",
    "react-jss": "^8.6.1",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sinon": "^7.2.2",
    "@lord-abbett/common-components": "1.0.24"
  },
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-enzyme": "^7.0.1",
    "jest-junit": "^5.2.0",
    "jest-sonar-reporter": "^2.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.3.1",
    "yarn": "^1.9.4"
  }
}
